<h1>Edit Profile</h1>
<%= @user.email %>
<%= form_for @user do |f| %>
	<% if @user.errors.any? %>
	  <div class='error_messages'>
	  	<h2> Form is invalid </h2>
	  	<ul>
	    	  <% @user.errors.full_messages.each do |message| %>
	    		<li><%= message %></li>
	    	  <% end %>
	  	</ul>
	  </div>
	<% end %>
	<%= f.label :zip_code %>
	<%= f.text_field :zip_code %>
	<%= f.submit %>
<% end %>